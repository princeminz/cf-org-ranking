{
  "name": "CF Contest Rating",
  "description": "Build an Extension!",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://codeforces.com/contest/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://cf-api.vercel.app/api"
  ],
  "action": {
    "default_title": "CF Contest Organisation Rating",
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png",
      "64": "icon_64.png",
      "128": "icon_128.png"
    }
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  }
  
}